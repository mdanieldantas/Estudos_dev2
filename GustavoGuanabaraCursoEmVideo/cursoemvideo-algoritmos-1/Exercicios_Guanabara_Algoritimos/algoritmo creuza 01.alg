algoritmo "creuza 01"

var

   anoAtual:inteiro
   anoNascimento:inteiro
   idadeAtual: inteiro

inicio
// Seção de Comandos
   escreva ("Em que ano estamos? ")
   leia (anoAtual)
   escreva ("Em que ano eu nasci? ")
   leia (anoNascimento)
   idadeAtual <- anoAtual - anoNascimento
   Escreva("Minha idade é: ",idadeAtual," anos.")


fimalgoritmo
